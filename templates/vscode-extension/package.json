{
    "publisher": "<author>",
    "name": "mori-extension-name",
    "displayName": "mori-extension-name",
    "version": "0.0.0",
    "private": true,
    "packageManager": "pnpm@10.4.1",
    "description": "<description>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "<your git url>"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/index.js",
    "icon": "res/icon.png",
    "files": [
        "LICENSE",
        "dist/*",
        "res/*"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "mori.hello",
                "title": "Hello Mori"
            }
        ],
        "configuration": {
            "title": "Mori Hello",
            "properties": {
                "mori.hello": {
                    "type": "boolean",
                    "default": true,
                    "description": "Show 'Hello Mori'"
                }
            }
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --external vscode",
        "dev": "tsup src/index.ts --external vscode --watch --sourcemap",
        "update": "vscode-ext-gen --output src/generated/meta.ts",
        "lint": "eslint .",
        "publish": "vsce publish --no-dependencies",
        "pack": "vsce package --no-dependencies",
        "typecheck": "tsc --noEmit",
        "test": "vitest"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@stylistic/eslint-plugin": "^4.2.0",
        "@types/node": "^22.13.4",
        "@types/vscode": "^1.97.0",
        "@vscode/vsce": "^3.2.2",
        "eslint": "^9.20.1",
        "reactive-vscode": "^0.2.10",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.26.1",
        "vitest": "^3.0.9",
        "vscode-ext-gen": "^1.0.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}